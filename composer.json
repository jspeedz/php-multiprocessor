{
  "name": "jspeedz/php-multiprocessor",
  "description": "Forking jobs in PHP to speed up processes",
  "keywords": [
    "forking",
    "multiprocess"
  ],
  "type": "library",
  "minimum-stability": "stable",
  "license": "MIT",
  "authors": [
    {
      "name": "jspeedz",
      "role": "Developer"
    }
  ],
  "autoload": {
    "psr-4": {
      "Jspeedz\\MultiProcessor\\": "src/"
    },
    "files": [
    ],
    "exclude-from-classmap": [
      "/Tests/"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Jspeedz\\MultiProcessor\\Tests\\": "src/Tests"
    }
  },
  "require": {
    "php": ">=7.1",
    "ext-pcntl": "*",
    "ext-posix": "*",
    "ext-json": "*"
  }
}
